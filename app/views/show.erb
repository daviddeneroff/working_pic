<div>
  <h3>Welcome <%= @user.username.capitalize %></h3>
</div>


  <h6>Here are all of your posts</h6>
  <% @user.posts.each do |post| %>
  Title: <%= post.title %>: <br>
    <img  class="photo" src="<%=post.photo_url%>"><br>
     <h4> Comments: </h4>
    <%post.comments.each do |comment|%>
     <%=comment.content%><br>
    <%end%>
  <%end%>
